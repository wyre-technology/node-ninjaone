{
  "name": "@wyre-technology/node-ninjaone",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "Comprehensive, fully-typed Node.js/TypeScript library for the NinjaOne/NinjaRMM API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ninjaone",
    "ninjarmm",
    "rmm",
    "msp",
    "api",
    "typescript",
    "remote-monitoring",
    "endpoint-management"
  ],
  "author": "Wyre Technology",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyre-technology/node-ninjaone.git"
  },
  "bugs": {
    "url": "https://github.com/wyre-technology/node-ninjaone/issues"
  },
  "homepage": "https://github.com/wyre-technology/node-ninjaone#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "msw": "^2.1.0",
    "semantic-release": "^24.2.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "dependencies": {}
}
